<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context=".Faculty.ArtsAndHumanities_Activity">

    <include
        layout="@layout/app_bar_layout"
        android:id="@+id/arts_humanities_toolbar"/>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/artsRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"
        android:layout_below="@+id/arts_humanities_toolbar"
        android:layout_alignParentTop="@+id/arts_humanities_toolbar"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        tools:ignore="MissingConstraints" />
</RelativeLayout>