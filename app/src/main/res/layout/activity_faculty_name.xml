<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context=".Activity.FacultyNameActivity">

  <include
    layout="@layout/app_bar_layout"
    android:id="@+id/faculty_main_toolbar"/>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/facultyRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"

        android:layout_below="@+id/faculty_main_toolbar"
        android:layout_alignParentTop="@+id/faculty_main_toolbar"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"

        tools:ignore="MissingConstraints" />


</RelativeLayout>